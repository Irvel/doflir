define main {
    A -> int;
    B -> int;
    C -> int;
    D -> int;
    A = 1;
    B = 1;
    C = 1;
    D = 1;

    if (A + B > D) {
        if (A < B) {
            A = 0;
            B = B + D;
        }
        else {
            C = A + B;
        }
    }
    else {
        A = B + C;
    }
    D = B + A * C;
}